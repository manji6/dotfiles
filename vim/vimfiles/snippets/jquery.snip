snippet     jqajax
abbr        jQuery.ajax(Object) #Ajax
	$.ajax({
		'url': '${1:url[Str]}',
		'data': ${2:getParam[Obj/Str]},
		'dataType': '${3:dataType[Str]}',
		'timeout': ${4:timeout(ms)[Num]},
		'success': function(data,success,xhr){
			${0:// Callback Action is here...}
		}
	});

